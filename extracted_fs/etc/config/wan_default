config pdp_conf_type default
	option connection_num 0
	option enable 1
	option type default
	option pdp_name default
	option ip_type 1
	option qci 0
	option apn internet
	option usr_2g3g any
	option pswd_2g3g any 
	option authtype_2g3g NONE
	option usr_4g any
	option pswd_4g any 
	option authtype_4g NONE
	option connect_mode 1
	option lte_default 1
	option auto_apn 1
	option data_on_roaming 1
	option mtu 1500
	option modifiable 0
